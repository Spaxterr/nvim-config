{
    "diagnostic.displayByAle": true,
    "diagnostic.enable": true,
    "diagnostic.floatConfig": {
        "rounded": true,
        "border": true,
        "winblend": 1,
        "borderhighlight": "Comment"
    },
    "dialog.rounded": true,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.enableMessageDialog": false,
    "coc.source.file.enable": true,
    "coc.source.around.enable": true,
    "suggest.floatConfig": {
        "rounded": true,
        "border": true,
        "winblend": 1,
        "borderhighlight": "Comment"
    },
    "signature.floatConfig": {
        "rounded": true,
        "border": true,
        "winblend": 1,
        "borderhighlight": "Type"
    },
    "suggest.selection": "recentlyUsed",
    "suggest.localityBonus": true,
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "suggest.insertMode": "insert",
    "diagnostic.checkCurrentLine": true,
    "snippets.ultisnips.pythonPrompt": false,
    "inlayHint.enable": false,
    "Lua.workspace.checkThirdParty": false,

    "python.linting.enabled": false,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.typeCheckingMode": "strict",
    "python.pythonPath": "python3",

    "java.jdt.ls.java.home": "/home/developer/.jenv/versions/21.0/",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.maven.enabled": true,
    "java.maven.downloadSources": true,
    "java.import.gradle.enabled": true,
    "java.format.enabled": false,
    "java.completion.enabled": true,
    "java.completion.favoriteStaticMembers": [],
    "java.jdt.ls.vmargs": "-Xmx12G -Xms1G -XX:+UseG1GC -XX:+UseStringDeduplication",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "java.autobuild.enabled": false,
    "java.progressReports.enabled": true,
    "java.trace.server": "off",
    "java.signatureHelp.enabled": true,

    "svelte.enable-ts-plugin": true,
    "svelte.plugin.typescript.enable": true,
    "svelte.plugin.css.enable": true,
    "svelte.plugin.svelte.format.enable": false,

    "tailwindCSS.enable": true,
    "tailwindCSS.suggestions": true,
    "tailwindCSS.colorDecorators": true,
    "tailwindCSS.includeLanguages": {
        "svelte": "html"
    },

    "rust-analyzer.cachePriming.enable": true,
    "rust-analyzer.updates.checkOnStartup": true,
    "rust-analyzer.inlayHints.typeHints.enable": true,
    "rust-analyzer.inlayHints.chainingHints.enable": true,
    "rust-analyzer.inlayHints.parameterHints.enable": true
}
