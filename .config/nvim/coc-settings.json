{
    "diagnostic.displayByAle": true,
    "diagnostic.enable": true,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.enableMessageDialog": false,
    "coc.source.file.enable": true,
    "coc.source.around.enable": true,
    "Lua.workspace.checkThirdParty": false,
    "inlayHint.enable": false,
    "suggest.noselect": false,
    "suggest.enablePreselect": false,
    "diagnostic.checkCurrentLine": true,
    "snippets.ultisnips.pythonPrompt": false,
    "svelte.enable-ts-plugin": true,

    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": ["--config", "./tox.ini", "--verbose"],
    "python.linting.pylintEnabled": false,
    "python.analysis.autoImportCompletions": true,
    "python.pythonPath": "python3",

    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.maven.enabled": true,
    "java.maven.downloadSources": true,
    "java.import.gradle.enabled": true,
    "java.format.enabled": true,
    "java.completion.enabled": true,
    "java.completion.favoriteStaticMembers": [],
    "java.jdt.ls.vmargs": "-Xmx4G -Xms1G -XX:+UseG1GC -XX:+UseStringDeduplication",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "java.autobuild.enabled": false,
    "java.saveActions.organizeImports": true,
    "java.progressReports.enabled": true,
    "java.trace.server": "off",
    "java.signatureHelp.enabled": false,

    "svelte.plugin.typescript.enable": true,
    "svelte.plugin.css.enable": true,
    "svelte.plugin.svelte.format.enable": false,

    "tailwindCSS.enable": true,
    "tailwindCSS.suggestions": true,
    "tailwindCSS.colorDecorators": true,
    "tailwindCSS.includeLanguages": {
        "svelte": "html"
    },

    "rust-analyzer.cachePriming.enable": true,
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
    "rust-analyzer.updates.checkOnStartup": true,
    "rust-analyzer.inlayHints.typeHints.enable": true
}
