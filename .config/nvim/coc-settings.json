{
    "suggest.noselect": false,
    "suggest.enablePreselect": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": false,

    "java.jdt.ls.vmargs": "-Xms1g -Xmx8g -XX:+UseG1GC -XX:+UseStringDeduplication -XX:MaxGCPauseMillis=200",
    "java.completion.enabled": true,
    "java.sharedIndexes.enabled": "on",
    "java.signatureHelp.enabled": true,
    "java.codeGeneration.generateComments": false,
    "java.referencesCodeLens.enabled": false,
    "java.completion.postfix.enabled": false,
    "java.completion.guessMethodArguments": false,
    "java.completion.matchCase": "off",
    "java.inlayHints.parameterNames.enabled": "none",

    "snippets.ultisnips.pythonPrompt": false,

    "prettier.enable": true,
    "prettier.semi": true,
    "prettier.useTabs": false,
    "prettier.tabWidth": 4,
    "prettier.printWidth": 110,
    "prettier.trailingComma": "es5",
    "prettier.singleQuote": true,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "python.analysis.autoImportCompletions": true,
    "inlayHint.enable": true,
    "Lua.workspace.checkThirdParty": false,

    "svelte.enable-ts-plugin": true,
    "svelte.plugin.css.enable": true,
    "svelte.plugin.html.enable": true,
    "svelte.plugin.typescript.enable": true,
    "svelte.plugin.html.completions.enable": true,
    "svelte.plugin.svelte.defaultScriptLanguage": "ts",
    "typescript.inlayHints.variableTypes.enabled": true,
    "svelte.ui.svelteKitFilesContextMenu.enable": "auto"
}
